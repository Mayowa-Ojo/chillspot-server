      "ts-jest": "^26.4.4",
      "ts-node": "^9.0.0",
      "tsconfig-paths": "^3.9.0",
      "tscpaths": "0.0.9",
      "tslint": "^6.1.3",
      "typescript": "^4.1.2"
   }
