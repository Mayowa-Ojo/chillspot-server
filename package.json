{
   "name": "chillspot-server",
   "version": "1.0.0",
   "description": "server for chillspot web app",
   "main": "dist/server.js",
   "scripts": {
      "start": "npm run build && node dist/server.js",
      "start:dev": "nodemon",
      "start:prod": "npm run build && node dist/server.js",
      "build": "rimraf ./dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
      "test": "cross-env NODE_ENV=test jest --detectOpenHandles"
   },
   "homepage": "https://github.com/Mayowa-Ojo/chillspot/server/#README",
   "repository": "github:Mayowa-Ojo/chillspot/server",
   "keywords": [
      "typescript",
      "koa",
      "koa2",
      "jwt",
      "helmet",
      "cors",
      "mongodb"
   ],
   "engines": {
      "node": "14.x.x",
      "npm": "6.x.x"
   },
   "author": "Mayowa Ojo",
   "license": "ISC",
   "dependencies": {
      "@koa/cors": "^3.1.0",
      "@koa/multer": "^3.0.0",
      "@koa/router": "^10.0.0",
      "@typegoose/typegoose": "^7.4.2",
      "aws-sdk": "^2.799.0",
      "bcrypt": "^5.0.0",
      "class-validator": "^0.12.2",
      "dotenv": "^8.2.0",
      "http-status-codes": "^2.1.4",
      "jsonwebtoken": "^8.5.1",
      "koa": "^2.13.0",
      "koa-bodyparser": "^4.3.0",
      "koa-helmet": "^6.0.0",
      "koa-jwt": "^4.0.0",
      "mongodb": "^3.6.3",
      "mongoose": "^5.10.15",
      "multer": "^1.4.2",
      "nanoid": "^3.1.20",
      "rimraf": "^3.0.2",
      "winston": "^3.3.3"
   },
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/jest": "^26.0.19",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/koa": "^2.11.6",
      "@types/koa-bodyparser": "^4.3.0",
      "@types/koa-helmet": "^6.0.1",
      "@types/koa__cors": "^3.0.2",
      "@types/koa__router": "^8.0.3",
      "@types/mongoose": "^5.10.1",
